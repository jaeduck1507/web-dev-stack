<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="m">
	<select id="member_list" resultType="vo.MemberVO">
		select * from member
	</select>
	
	<insert id="member_register" parameterType="vo.MemberVO">
		insert into member values(
			seq_member_idx.nextVal,
			#{name},
			#{id},
			#{pwd},
			#{email},
			#{addr}
		)
		
	</insert>
	
	<select id="id_check" resultType="vo.MemberVO" parameterType="String">
		select * from member where id = #{id}
	</select>
	
	<delete id="member_delete" parameterType="vo.MemberVO">
		delete from member where idx= #{idx}
	</delete>

	<select id="modify_form" resultType="vo.MemberVO" parameterType="int">
		select * from member where idx=#{idx}
	</select>
	
	
</mapper>












